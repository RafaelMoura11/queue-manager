{
  "name": "queue-manager",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "tsc": "npx tsc",
    "db:init": "npx sequelize-cli db:create && npx sequelize-cli db:migrate",
    "db:reset": "npx sequelize-cli db:drop && npx sequelize-cli db:create && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
    "init": "node ./build/index.js",
    "start": "npm run tsc && npm run db:init && npm run init",
    "dev": "ts-node-dev ./src/index.ts"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "mysql2": "^3.9.7",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.37.3",
    "sequelize-typescript": "^2.1.6",
    "venom-bot": "^5.1.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.12.12",
    "@types/sequelize": "^4.28.20",
    "bcryptjs": "^2.4.3",
    "globals": "^15.3.0",
    "jsonwebtoken": "^9.0.2",
    "nodemon": "^3.1.0",
    "sequelize-cli": "^6.6.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5"
  }
}
